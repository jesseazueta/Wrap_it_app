
<%=image_tag(current_user.image, height:'175px', width: '175px',class: "item_pic")%>
<h1> <%= link_to current_user.first_name + " " + current_user.last_name, current_user %>'s on Wrap-It!</h1>
<h1>People on Wrap it!</h1>

<p>
  <% @list.each do |letter, users| %>
  <h1><%=letter.upcase%></h1>
  <% users.each do |user| %>
  <% unless user == current_user || current_user.friends.include?(user) %>
  <ul>
    <li>
      <%=image_tag(user.image, height:'175px', width: '175px',class: "item_pic")%>
      <strong><%=user.first_name + " " + user.last_name%></strong>
      <%= link_to "Add Friend", friendships_path(:friend_id => user), :method => :post%>
    </li>
  </ul>
  <% end %>
</br>
<% end %>
<% end %>
</p>
