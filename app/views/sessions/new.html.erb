<div class="header">
  <h1 class=top_bar>Welcome Back! We've Been Expecting you!</h1>
</div>

<h1 id="new_session">Log In</h1>
<div class="login_form">
  <%= form_tag sessions_path do %>
  <div class="field">
    <!-- <%= label_tag :email %><br> -->
    <%= text_field_tag :email, nil, placeholder: "Email:joe@gmail.com" %>
  </div>
  <div class="field">
    <!-- <%#= label_tag :password %><br> -->
    <%= password_field_tag :password,nil, placeholder: "Password" %>
  </div>
  <div class="actions">
    <%= submit_tag "Log In", :class => 'upload'%>
  </div>
  <% end %>
</div>
<footer>
  <div class="footer_message">
    That's What We Are Here For...
  </div>

</footer>
